-- !result

with model as (
    select wls_train(Y, [X1, X2, X3], weight, false) as params
    from wls_test_tbl
)
select round(avg(eval_ml_method(model.params, [X1, X2, X3])), 5) from wls_test_tbl, model;
-- result:
351.9874
-- !result

