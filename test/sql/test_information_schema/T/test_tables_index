-- name: test_tables_index
CREATE TABLE `tables_index_test`
(
    k1 TINYINT,
    k2 int(11) DEFAULT "1",
    v1 CHAR(10) REPLACE,
    v2 INT SUM,
    INDEX k1_idx (k1) USING BITMAP COMMENT 'xxxxxx',
    INDEX k2_idx (k2) USING BITMAP COMMENT 'xxxxxx'
)
ENGINE = olap
AGGREGATE KEY(k1, k2)
COMMENT "my first starrocks table"
DISTRIBUTED BY HASH(k1)
PROPERTIES (
"replication_num" = "1"
);
select TABLE_NAME, INDEX_NAME from INFORMATION_SCHEMA.TABLES_INDEX where table_name like '%tables_index_test%' order by index_name;
