-- name: test_probability_distributions

select percent_point_function(x / 100.0)
from TABLE(generate_series(1, 99, 1)) as tbl(x);
select gauss_error_function('inf');
select percent_point_function('nan');
select gauss_error_function(NULL);
select percent_point_function(10.01);
select percent_point_function(1.01);
select percent_point_function(1.00);
select percent_point_function(0.0);
select percent_point_function(-0.01);
select percent_point_function(-1);
select gauss_error_function(x / 1000.0)
from TABLE(generate_series(-5000, 5000, 66)) as tbl(x);
select gauss_error_function('inf');
select gauss_error_function('nan');
select gauss_error_function(NULL);
